{
  "name": "asperitas",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^8.0.4",
    "react-redux": "^8.0.5",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.0",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.4.2",
    "remark-breaks": "^3.0.2",
    "styled-components": "^5.3.6"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!(react-markdown))/"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "now-build": "npm run build && mv build dist",
    "test": "react-scripts test",
    "test:docker": "sudo docker compose exec client npm run test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "ignorePatterns": [
      "node_modules",
      "/**/*.js",
      "/**/*.jsx",
      "/**/*.ts",
      "/**/*.tsx"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-transition-group": "^4.4.5",
    "@types/redux-form": "^7.4.2",
    "@types/styled-components": "^5.1.26",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "enzyme": "^3.11.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-check": "^6.0.1",
    "prettier": "^2.8.1",
    "redux-mock-store": "^1.5.4",
    "ts-migrate": "^0.1.35",
    "typescript": "^4.9.4"
  }
}
